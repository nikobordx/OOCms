<%
Database: news,DESCORDER[id]
Column: Title,title
Column: Contents,contents
LineCount: NewsCount
%>

{[ i: 0..NewsCount ]
    {[ Title(i) != NULL ]
    <div class="cadre">
        <div class="titre">
            <%Show: Title(i)%>
        </div>
        <div class="marge_interne">
            <br/>
            <%Show: Contents(i)%>
        </div>
    </div>
    <br/><br/>
    }
}